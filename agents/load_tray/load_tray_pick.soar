# Propose to pick up an object to put on the tray
sp {topstate*propose*pick-up
   (state <s> ^superstate nil
              ^agent-name load-tray
              ^svs.spatial-scene <sc>
              ^object-list <ol>
              ^done-loading <d>
             -^held-object)
   (<ol> ^first <oid>)
   (<d> -^object <oid>)
   (<sc> ^child.id <oid>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name pick-up
        ^use-objective <j>
        ^object-id <oid>)
   (<j> ^name waypoints
        ^target-object none
        ^direction min)
}