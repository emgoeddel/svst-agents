# Propose to pick up the first object to put on the tray
sp {topstate*propose*pick-up*first
   (state <s> ^superstate nil
              ^agent-name load-tray
              ^svs.spatial-scene <sc>
              ^object-list <ol>
              ^done-loading <d>
             -^held-object)
   (<ol> ^first <oid>)
   (<d> -^object <oid>)
   (<sc> ^child.id <oid>)
-->
   (<s> ^operator <o> +)
   (<o> ^name pick-up
        ^objectives <objs>
        ^object-id <oid>)
   (<objs> ^first <obf>)
#           ^second <obs>)
   (<obf> ^name execution-time
          ^parameters <pmsf>)
   (<pmsf> ^direction min)
#   (<obs> ^name execution-time
#          ^parameters <pmss>)
#   (<pmss> ^direction min)
}

