sp {execute-request*propose*make-execute
   (state <s> ^superstate nil
              ^agent-name random-reach
              ^svs.motor.trajectories <t>)
   (<t> ^set <st>
        ^nothing no)
   (<st> ^command-id <sid>
         ^trajectory.id <tid>)
-->
   (<s> ^operator <o> +)
   (<o> ^name make-execute
        ^trajectory-set <sid>
        ^trajectory-id <tid>)
}

sp {execute-request*apply*make-execute
   (state <s> ^superstate nil
              ^agent-name random-reach
              ^svs <v>
              ^operator <op>)
   (<op> ^name make-execute
         ^trajectory-set <sid>
         ^trajectory-id <tid>)
   (<v> ^command <c>)
-->
   (<c> ^execute-trajectory <x>)
   (<x> ^set-id <sid>
        ^trajectory-id <tid>)
}